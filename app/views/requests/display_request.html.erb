<div class="container" >
      <div class="row">
        <div class="col-sm-6 col-sm-offset-3 form-box wow fadeInUp content-box text-center">
          <h1>Your request is being processed. We'll get back to you soon.</h1>

          <% if @request.client_id != Client.last.id %>
          <h2>Click Enter to Complete the Process.</h2>

           <%= link_to "Enter", [:add, @request], method: :patch, 
           					data: { confirm: "Thank You for using our service. We will get back to you as soon as possible." } %>
          <% else %>
           <%= link_to "Back", root_path %>
          <% end %>
        </div>
      </div>
    </div>


