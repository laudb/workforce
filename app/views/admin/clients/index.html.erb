<div class="admin-page">
	<div class="container">
		<div class="row">
			<div class="col-sm-9 col-sm-offset-1 form-box">

				<h1>A list of Clients</h1>

				<% @clients.each do |client| %>
				<h3><%= client.fullname  %> <%= link_to fa_icon("eye") + "View", admin_client_path(client), class: "btn btn-primary" %></h3>
				<% end %>


			</div>
		</div>
	</div>
</div>