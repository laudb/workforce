<div class="container" >
      <div class="row">
      <!-- <div class="col-sm-6 form-box wow fadeInUp content-box"></div> -->

       <div class="col-sm-5 col-sm-offset-3 form-box form-content signUpIn">
          <h2 class="text-center">Log in</h2>

          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <div class="field">
              <%= f.label :email, :class=>"login-label" %><br />
              <%= f.email_field :email, autofocus: true, :class=>"form-control" %>
            </div>

            <div class="field">
              <%= f.label :password, :class=>"login-label" %><br />
              <%= f.password_field :password, autocomplete: "off", :class=>"form-control" %>
            </div>

            <% if devise_mapping.rememberable? -%>
              <div class="field">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me, :class=>"login-label" %>
              </div>
            <% end -%>

            <div class="actions">
              <%= f.submit "Log in", :class=>"btn-block btn-lg btn-login" %>
            </div>
          <% end %>

          <%= render "clients/shared/links" %>
    </div>
  </div>
</div>